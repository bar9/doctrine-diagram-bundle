{
  "name": "jawira/doctrine-diagram-bundle",
  "type": "symfony-bundle",
  "description": "\ud83d\udcd0 Symfony Bundle to generate database diagrams",
  "keywords": [
    "symfony-bundle",
    "diagram",
    "doctrine",
    "database"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jawira Portugal"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "doctrine/orm": "^2.5",
    "doctrine/persistence": "^2.2",
    "jawira/db-draw": "^1.2",
    "jawira/plantuml-client": "^1.0",
    "symfony/console": "^4.0",
    "symfony/filesystem": "^4.4",
    "symfony/framework-bundle": "^4.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.15",
    "jawira/skeleton": "^2.6",
    "phpstan/phpstan": "^0.12.99"
  },
  "config": {
    "platform-check": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Jawira\\DoctrineDiagramBundle\\": "src/"
    }
  },
  "support": {
    "issues": "https://github.com/jawira/doctrine-diagram-bundle/issues"
  }
}
